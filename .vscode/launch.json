{
	"version": "0.2.0",
	"configurations": [
		{
			"type": "node",

			"request": "launch",
			"name": "Run npm dev with Bun",
			"runtimeExecutable": "npm",
			"runtimeArgs": ["run", "dev"],
			"console": "integratedTerminal",
			"internalConsoleOptions": "neverOpen",
			"skipFiles": ["<node_internals>/**"]
		}
	]
}
